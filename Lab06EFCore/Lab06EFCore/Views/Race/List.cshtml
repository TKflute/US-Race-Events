@model RaceListViewModel

@foreach (Race r in Model.Races)
{
    @Html.Partial("RaceSummary", r)
}

<div page-model="@Model.PagingInfo" page-action="List"
     page-classes-enabled="true" page-class="btn"
     page-class-normal="btn-default"
     page-class-selected="btn-primary"
     class="btn-group pull-right">
</div>