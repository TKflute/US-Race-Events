@model IEnumerable<Runner>

<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>Runners</title>
    <link href="~/bootstrap3/dist/css/bootstrap.css" rel="stylesheet" />
</head>
<body>
    <h1>Runners</h1>
    <table class="table table-sm table-striped table-bordered">
        <tr>
            <td>First Name</td>
            <td>Last Name</td>
            <td>Gender</td>
            <td>Age</td>
        </tr>
        @foreach (Runner r in Model)
        {
            <tr>
                <td>@r.FirstName</td>
                <td>@r.LastName</td>
                <td>@r.Gender</td>
                <td>@r.Age</td>
            </tr>
        }
    </table>
</body>
</html>


